{
    "name": "ornicar/message-bundle",
    "type": "symfony-bundle",
    "description": "Provides user-to-user messaging features for your Symfony2 application.",
    "keywords": ["symfony", "message", "messaging"],
    "homepage": "https://github.com/ornicar/OrnicarMessageBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Thibault Duplessis",
            "email": "thibault.duplessis@gmail.com",
            "homepage": "http://ornicar.github.com"
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "symfony/framework-bundle": "2.1.*",
        "symfony/twig-bundle": "2.1.*"
    },
    "suggest": {
        "doctrine/doctrine-bundle": "dev-master",
        "doctrine/mongodb-odm-bundle": "dev-master"
    },
    "autoload": {
        "psr-0": { "Ornicar\\OrnicarMessageBundle": "" }
